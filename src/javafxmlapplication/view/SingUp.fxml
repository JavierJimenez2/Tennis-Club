            <VBox fx:id="intPane">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="returnButton" mnemonicParsing="false" onAction="#returnAction">
                           <graphic>
                              <ImageView fitHeight="14.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@css/img/icons/return.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="30.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <Text fill="#1f8918" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1" text="GreenBall" textAlignment="CENTER" wrappingWidth="598.7831211090088">
                     <font>
                        <Font name="Courier New Bold" size="43.0" />
                     </font>
                  </Text>
                  <Text fill="#1f8918" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h4" text="Register" textAlignment="CENTER" wrappingWidth="598.7831211090088">
