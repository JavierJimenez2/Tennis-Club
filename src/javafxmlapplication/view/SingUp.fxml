                        <Font name="Courier New Bold" size="28.0" />
                     </font>
                  </Text>
                  <VBox prefHeight="360.0" prefWidth="703.0">
                     <children>
                        <HBox prefHeight="310.0" prefWidth="703.0" VBox.vgrow="ALWAYS">
                           <children>
                              <VBox fx:id="fieldsInputs" prefHeight="244.0" prefWidth="352.0" spacing="10.0">
                                 <children>
                                    <TextField id="text-field" fx:id="name" promptText="Name" styleClass="text-field">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </TextField>
                                    <TextField id="text-field" fx:id="lastname" promptText="Last Name" styleClass="text-field">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </TextField>
                                    <TextField id="text-field" fx:id="username" promptText="Username">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </TextField>
                                    <PasswordField id="password-field" fx:id="password" promptText="Password">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </PasswordField>
                                    <Separator fx:id="separator" prefWidth="200.0" />
                                    <TextField id="text-field" fx:id="telephone" promptText="Telephone Number">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </TextField>
                                    <TextField id="text-field" fx:id="creditcard" prefHeight="26.0" prefWidth="246.0" promptText="Credit Card Number (Optional)">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </TextField>
                                    <TextField id="text-field" fx:id="csc" prefHeight="26.0" prefWidth="221.0" promptText="CSC (optional)">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </TextField>
                                 </children>
                                 <HBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </HBox.margin>
                              </VBox>
                              <VBox alignment="CENTER" prefHeight="182.0" prefWidth="302.0">
                                 <children>
                                    <VBox alignment="CENTER" prefHeight="191.0" prefWidth="160.0">
                                       <children>
                                          <ImageView fx:id="profileImage" fitHeight="98.0" fitWidth="89.0" pickOnBounds="true" preserveRatio="true" />
                                          <Button id="selectButton" fx:id="selection" mnemonicParsing="false" onAction="#selectionButton" text="Select">
                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </Button>
                                       </children>
                                    </VBox>
                                 </children>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </VBox>
                           </children>
                        </HBox>
                        <HBox fx:id="buttonsZone" alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="694.0" VBox.vgrow="NEVER">
                           <children>
                              <Button id="submitButton" fx:id="register" alignment="CENTER" mnemonicParsing="false" onAction="#registerButton" text="Submit" textFill="WHITE">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font name="System Bold" size="15.0" />
                                 </font>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" left="30.0" right="30.0" top="10.0" />
                     </VBox.margin>
                  </VBox>
               </children>
            </VBox>
         </children>
         <HBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
